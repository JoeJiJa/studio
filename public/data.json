  "questionOfTheDay": {
    "question": "A 55-year-old male presents with a sudden onset of 'the worst headache of his life.' A non-contrast CT scan of the head is performed. Which of the following is the most likely diagnosis?",
    "options": [
      "Ischemic Stroke",
      "Migraine with Aura",
      "Subarachnoid Hemorrhage",
      "Tension Headache"
    ],
    "answerIndex": 2
  },
  "caseStudies": [
    {
      "id": "cs001",
      "title": "A Case of Chest Pain",
      "scenario": "A 62-year-old male with a history of hypertension and hyperlipidemia presents to the emergency department with a 2-hour history of crushing substernal chest pain radiating to his left arm. He is diaphoretic and short of breath.",
      "question": "An ECG is performed and shows ST-segment elevation in leads V1-V4. What is the most likely diagnosis?",
      "options": [
        "Pericarditis",
        "Anterior Wall Myocardial Infarction",
        "Aortic Dissection",
        "Pulmonary Embolism"
      ],
      "correctAnswer": 1,
      "explanation": "ST-segment elevation in the anterior leads (V1-V4) is a classic sign of an acute anterior wall myocardial infarction, typically caused by occlusion of the Left Anterior Descending (LAD) artery."
    },
    {
      "id": "cs002",
      "title": "A Young Patient with a Rash",
      "scenario": "A 7-year-old child is brought to the clinic with a 3-day history of low-grade fever, cough, and a maculopapular rash. The rash started on the face and has now spread to the trunk and extremities. You notice small white spots on the buccal mucosa.",
      "question": "What are these characteristic white spots called, and what is the most probable diagnosis?",
      "options": [
        "Forchheimer spots; Rubella",
        "Koplik's spots; Measles (Rubeola)",
        "Pastia's lines; Scarlet Fever",
        "Slapped cheek appearance; Erythema Infectiosum (Fifth Disease)"
      ],
      "correctAnswer": 1,
      "explanation": "Koplik's spots, which are small, white spots on the buccal mucosa, are pathognomonic for Measles (Rubeola). The clinical presentation of fever, cough, coryza, and a descending maculopapular rash further supports this diagnosis."
    }
  ],
