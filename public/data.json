  "caseStudies": [
    {
      "id": "case-001",
      "title": "Case of the Short-of-Breath Smoker",
      "scenario": "A 65-year-old male with a 40-pack-year smoking history presents with a chronic cough, progressive shortness of breath, and wheezing. On examination, he has a barrel chest and pursed-lip breathing.",
      "question": "What is the most likely diagnosis?",
      "options": [
        "Asthma",
        "Chronic Obstructive Pulmonary Disease (COPD)",
        "Pulmonary Fibrosis",
        "Lung Cancer"
      ],
      "correctAnswer": 1,
      "explanation": "The patient's age, extensive smoking history, and classic symptoms (chronic cough, dyspnea) and signs (barrel chest, pursed-lip breathing) are hallmark features of COPD."
    },
    {
        "id": "case-002",
        "title": "The Anxious Young Woman",
        "scenario": "A 24-year-old female presents to the emergency department with palpitations, sweating, and a feeling of impending doom that started 20 minutes ago. She has no significant past medical history. An ECG and initial blood work are normal.",
        "question": "What is the most likely diagnosis?",
        "options": [
          "Myocardial Infarction",
          "Pulmonary Embolism",
          "Panic Attack",
          "Thyroid Storm"
        ],
        "correctAnswer": 2,
        "explanation": "Given her age, the acute onset of classic autonomic arousal symptoms (palpitations, sweating), the psychological component ('impending doom'), and normal initial workup, a panic attack is the most likely diagnosis. While other serious conditions must be ruled out, her presentation is classic for panic disorder."
      }
  ],
