  "caseStudies": [
    {
      "id": "case-1",
      "title": "65-Year-Old with Chest Pain",
      "scenario": "A 65-year-old male with a history of hypertension and smoking presents to the emergency department with a 2-hour history of crushing substernal chest pain radiating to his left arm.",
      "question": "An ECG is performed and shows ST-segment elevation in leads II, III, and aVF. What is the most likely diagnosis?",
      "options": [
        "Acute Pericarditis",
        "Pulmonary Embolism",
        "Inferior Wall Myocardial Infarction",
        "Aortic Dissection"
      ],
      "correctAnswer": 2,
      "explanation": "ST-segment elevation in leads II, III, and aVF is the classic sign of an inferior wall myocardial infarction, typically caused by occlusion of the right coronary artery."
    }
  ],
